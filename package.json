{
  "name": "@vinicunca/unocss-preset-vinicunca",
  "type": "module",
  "version": "0.2.8",
  "packageManager": "pnpm@9.12.3",
  "description": "Opinionated UnoCSS preset",
  "author": "praburangki<https://github.com/praburangki>",
  "license": "MIT",
  "homepage": "https://github.com/vinicunca/unocss-preset-vinicunca#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinicunca/unocss-preset-vinicunca.git"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/unocss-preset-vinicunca/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-preset-animation",
    "unocss-preset-fluid"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "vite playground",
    "stub": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp package.json",
    "publish:ci": "pnpm publish --access public --no-git-checks",
    "test": "vitest",
    "test:update": "vitest --update",
    "build:docs": "pnpm --filter @vinicunca/unocss-docs build"
  },
  "dependencies": {
    "@unocss/preset-icons": "^0.63.6",
    "@unocss/preset-mini": "^0.63.6",
    "@unocss/preset-uno": "^0.63.6",
    "@vinicunca/perkakas": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^22.8.2",
    "@vinicunca/eslint-config": "^2.12.0",
    "bumpp": "^9.8.0",
    "eslint": "^9.13.0",
    "typescript": "5.6.3",
    "unbuild": "^3.0.0-rc.11",
    "unocss": "^0.63.6",
    "vitest": "^2.1.4"
  }
}
