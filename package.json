{
  "name": "@vinicunca/unocss-preset",
  "type": "module",
  "version": "1.36.0",
  "packageManager": "pnpm@10.22.0",
  "description": "Opinionated UnoCSS preset",
  "author": "praburangki<https://github.com/praburangki>",
  "license": "MIT",
  "homepage": "https://github.com/vinicunca/unocss-preset-vinicunca#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinicunca/unocss-preset-vinicunca.git"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/unocss-preset-vinicunca/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-preset-animation",
    "unocss-preset-fluid"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp -r",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:update": "vitest --update",
    "play": "pnpm -C playground run dev",
    "taze": "taze major -wIrl",
    "typecheck": "tsc --noEmit",
    "publish:ci": "pnpm publish --access public --no-git-checks"
  },
  "peerDependencies": {
    "@vinicunca/perkakas": "catalog:",
    "unocss": "catalog:"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@types/node": "catalog:",
    "@unocss/core": "catalog:",
    "@unocss/preset-icons": "catalog:",
    "@unocss/preset-mini": "catalog:",
    "@unocss/preset-typography": "catalog:",
    "@unocss/preset-web-fonts": "catalog:",
    "@unocss/preset-wind4": "catalog:",
    "@unocss/transformer-directives": "catalog:",
    "@unocss/transformer-variant-group": "catalog:",
    "@vinicunca/eslint-config": "catalog:",
    "@vinicunca/perkakas": "catalog:",
    "@vinicunca/unocss-preset": "workspace:*",
    "bumpp": "catalog:",
    "defu": "catalog:",
    "eslint": "catalog:",
    "lint-staged": "catalog:",
    "magic-string": "catalog:",
    "postcss": "catalog:",
    "postcss-js": "catalog:",
    "prettier": "catalog:",
    "simple-git-hooks": "catalog:",
    "taze": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
